{{#each enums}}
interface {{@key}} {
  {{#each this}}
  const long {{{@key}}} = {{{this}}};
  {{/each}}
};
{{/each}}

interface A {
{{#each defs}}
  const long {{{@key}}} = {{{this}}};
{{/each}}
};

interface B {
{{#each funcs}}
  {{{this.type}}} {{{@key}}}({{{this.args}}});
{{/each}}
};

{{#each typedefs}}
{{{this.type}}} {{{@key}}}({{{this.args}}});
{{/each}}

{{#each structs}}
interface {{@key}} {
  {{#each this}}
  attribute {{{this}}} {{@key}};
  {{/each}}
};
{{/each}}
